{
  "name": "resizable-panes-react",
  "version": "2.0.9",
  "description": "A simple library for resizing elements in the browser",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.cjs.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs.js",
    "import": "./lib/index.cjs.js"
  },
  "scripts": {
    "build-w": "webpack --config configs/webpack.dev.js",
    "build-prod-w": "webpack --config configs/webpack.prod.js",
    "build-watch-w": "webpack --watch --config configs/webpack.dev.js",
    "build": "rimraf lib && rollup -c configs/rollup.config.dev.mjs",
    "build-prod": "rimraf lib && rollup -c configs/rollup.config.prod.mjs",
    "build-watch": "rollup -c configs/rollup.config.dev.mjs -w",
    "link-local": "npm link",
    "start-w": "npm-run-all --serial  build-watch-w link-local",
    "start": "npm-run-all --serial  build-watch link-local",
    "predeploy": "npm-run-all --serial  build-prod",
    "deploy": "npm publish",
    "test-clean":  "rd /s /q coverage .nyc_output>nul 2>&1|echo.>nul",
    "pretest": "npm run test-clean",
    "pretest:open": "npm run test-clean",
    "test:open": "cypress open --component --config-file cypress.config.ts browser --browser chrome",
    "test": "cypress run --component --config-file cypress.config.ts browser --browser chrome",
    "lint": "eslint ./src",
    "test:cloud": "cypress run --component --config-file cypress.config.ts browser --browser chrome"
  },
  "keywords": [
    "resizable",
    "resizable react",
    "resizable div",
    "resizable div react",
    "resizable panel",
    "resizable panel react",
    "resizable panes",
    "resizable panes react",
    "resizable container",
    "resizable container react",
    "resizable panes react",
    "split panes",
    "split panes react",
    "multi split",
    "multi split react",
    "resize",
    "resize react"
  ],
  "author": "bipankishore@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.24.1",
    "@cypress/code-coverage": "^3.12.34",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "cypress": "^13.7.3",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "fork-ts-checker-notifier-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BipanKishore/resizable-panes-react.git"
  },
  "bugs": {
    "url": "https://github.com/BipanKishore/resizable-panes-react/issues",
    "email": "bipankishore@gmail.com"
  },
  "homepage": "https://bipankishore.github.io/resizable-panes-demo/"
}
